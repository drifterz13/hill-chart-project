{
  "name": "todoi",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --hot server/index.ts",
    "start:prod": "NODE_ENV=production cd dist && bun index.js",
    "build": "bun run build.ts",
    "db:gen": "bun run server/scripts/gen-migration.ts",
    "db:migrate": "bun run server/scripts/migrate.ts",
    "db:drop": "bun run server/scripts/drop-db.ts",
    "db:seed": "bun run server/scripts/seed.ts",
    "db:reset": "bun db:drop && bun db:migrate && bun db:seed"
  },
  "devDependencies": {
    "@types/react-dom": "^19.2.2",
    "daisyui": "^5.4.7"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "bun-plugin-tailwind": "^0.1.2",
    "dayjs": "^1.11.19",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.5",
    "swr": "^2.3.6",
    "tailwindcss": "^4.1.17"
  }
}
